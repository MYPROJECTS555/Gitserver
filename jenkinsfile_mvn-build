pipeline
{
    agent any
    tools
    {
        jdk 'java-11'
        maven 'maven'
    }
    stages
    {
        stage('code-checkout')
        {
            steps
            {
                git branch:'main',url:'https://github.com/ManojKRISHNAPPA/test-1.git'
            }
        }
        stage('mvn compile')
        {
            steps
            {
                sh 'mvn compile'
            }
        }
        stage('mvn-install')
        {
            steps
            {
                sh 'mvn clean install'
            }
        }
    }
}
